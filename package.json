{
  "name": "travel-green",
  "version": "0.1.0",
  "description": "Log carbon-reducing activities",
  "main": "server.js",
  "private": true,
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.6",
    "express": "^4.17.1",
    "express-validator": "^6.13.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.13"
  },
  "scripts": {
    "start": "node server.js",
